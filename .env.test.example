# Local Mode (always enabled, no config needed)

# Remote IP Mode (optional)
REMOTE_SERVER_ADDRESS=192.168.1.100:3100
REMOTE_API_KEY=your-api-key-here

# Gateway Mode (Default)
GATEWAY_SECRET=test-gateway-secret
GATEWAY_API_KEY=your-gateway-api-key

# SOCKS5 Proxy (optional, for Gateway mode)
SOCKS5_PROXY_URL=socks5://127.0.0.1:1080
SOCKS5_PROXY_USER=proxyuser
SOCKS5_PROXY_PASS=proxypass

# Gateway Backend 1 (optional - for testing multiple gateways)
# Example: Production gateway backend
GATEWAY1_URL=ws://gateway1.example.com:3200
GATEWAY1_SECRET=gateway1-secret
GATEWAY1_API_KEY=gateway1-api-key
GATEWAY1_BACKEND_ID=backend-123  # Optional: Pre-configured backend ID
GATEWAY1_PROXY_URL=socks5://proxy1.example.com:1080
GATEWAY1_PROXY_USER=proxy1user
GATEWAY1_PROXY_PASS=proxy1pass

# Gateway Backend 2 (optional - for testing multiple gateways)
# Example: Staging gateway backend
GATEWAY2_URL=ws://gateway2.example.com:3201
GATEWAY2_SECRET=gateway2-secret
GATEWAY2_API_KEY=gateway2-api-key
GATEWAY2_BACKEND_ID=backend-456  # Optional: Pre-configured backend ID
GATEWAY2_PROXY_URL=socks5://proxy2.example.com:1080
GATEWAY2_PROXY_USER=proxy2user
GATEWAY2_PROXY_PASS=proxy2pass

# ========================================
# Same Gateway - Multiple Backends
# ========================================
# Use case: Multiple backend servers registered to the SAME gateway
# Example: User's laptop + desktop both accessible through one gateway
#
# Backend A: Personal laptop
GATEWAY_BACKEND_A_ID=backend-laptop-123
GATEWAY_BACKEND_A_KEY=laptop-api-key
GATEWAY_BACKEND_A_PROXY_URL=socks5://proxy-laptop.example.com:1080
GATEWAY_BACKEND_A_PROXY_USER=laptopuser
GATEWAY_BACKEND_A_PROXY_PASS=laptoppass

# Backend B: Work desktop
GATEWAY_BACKEND_B_ID=backend-desktop-456
GATEWAY_BACKEND_B_KEY=desktop-api-key
GATEWAY_BACKEND_B_PROXY_URL=socks5://proxy-desktop.example.com:1080
GATEWAY_BACKEND_B_PROXY_USER=desktopuser
GATEWAY_BACKEND_B_PROXY_PASS=desktoppass

# Note: Both Backend A and B use the same GATEWAY_SECRET and gateway server (localhost:3200)
# but have different backend IDs and API keys
